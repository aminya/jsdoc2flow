{
  "name": "jsdoc2flow",
  "version": "1.0.0",
  "description": "Convert JSDoc comments into Flow/Typescript annotations",
  "main": "src/index.js",
  "scripts": {
    "format": "prettier --write .",
    "postversion": "git push --tags && npm publish && git push origin master",
    "test": "mocha \"test/**/*-tests.js\"",
    "lint": "eslint . --fix"
  },
  "type": "commonjs",
  "prettier": "prettier-config-atomic",
  "dependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "awilix": "7.0.3",
    "commander": "9.4.0",
    "comment-parser": "^1.3.1",
    "doctrine": "3.0.0",
    "eslint": "8.22.0",
    "espree-attachcomment": "^7",
    "fs-extra": "10.1.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "eslint-config-atomic": "^1.18.1",
    "eslint-visitor-keys": "^3.3.0",
    "mocha": "10.0.0",
    "prettier-config-atomic": "^3.0.10",
    "should": "13.2.3"
  },
  "bin": "./bin/cli.js",
  "files": [
    "bin",
    "src",
    "vendor"
  ],
  "repository": "https://github.com/aminya/jsdoc2flow",
  "keywords": [
    "jsdoc2flow",
    "js2flow",
    "js-to-flow",
    "jsdoc-to-flow",
    "js-to-ts",
    "js-to-typescript",
    "jsdoc",
    "flow",
    "js2typescript",
    "js2ts",
    "typescript",
    "ts",
    "dts",
    "dts-gen"
